{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "transaction_id": {
            "type": "string"
        },
        "transaction_date": {
            "type": "string",
            "format": "date-time"
        },
        "amount": {
            "type": "number"
        },
        "currency": {
            "type": "string"
        },
        "transaction_status": {
            "type": "string"
        },
        "transaction_type": {
            "type": "string"
        },
        "order_id": {
            "type": "string"
        },
        "item_id": {
            "type": "string"
        }
    },
    "required": ["transaction_id", "transaction_date", "amount", "currency", "transaction_status", "transaction_type"]
}
